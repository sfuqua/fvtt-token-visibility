{
    "name": "fvtt-token-visibility",
    "version": "0.0.1",
    "private": true,
    "description": "Plugin module for Foundry VTT that reveals tokens seen by one player to all other players.",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf .tsbuildinfo module/**/*.js module/strings",
        "format": "prettier --write \"./**/*.{js,json,md,ts}\"",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest --colors"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sfuqua/fvtt-token-visibility.git"
    },
    "author": "Steven Fuqua",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sfuqua/fvtt-token-visibility/issues"
    },
    "homepage": "https://github.com/sfuqua/fvtt-token-visibility#readme",
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.0",
        "foundry-pc-types": "gitlab:foundry-projects/foundry-pc/foundry-pc-types",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    }
}
